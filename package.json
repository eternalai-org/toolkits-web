{
    "name": "@eternalai-org/toolkits-web",
    "version": "1.0.0",
    "description": "A CLI tool to install AI assistant templates for web developers",
    "main": "dist/index.js",
    "bin": {
        "toolkits-web": "./bin/cli.js"
    },
    "files": [
        "dist",
        "bin",
        "templates"
    ],
    "scripts": {
        "build": "bun build src/index.ts --outdir dist --target node --format esm && tsc --emitDeclarationOnly",
        "build:tsc": "tsc",
        "dev": "bun --watch src/index.ts",
        "prepublishOnly": "bun run build",
        "test:local": "./scripts.sh test",
        "publish:npm": "./scripts.sh publish",
        "release": "./scripts.sh full",
        "link": "bun link",
        "unlink": "bun unlink"
    },
    "keywords": [
        "cli",
        "template",
        "ai-assistant",
        "claude",
        "cursor",
        "windsurf",
        "antigravity",
        "copilot"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "commander": "^11.1.0",
        "chalk": "^4.1.2",
        "inquirer": "^8.2.6",
        "fs-extra": "^11.2.0",
        "ora": "^5.4.1"
    },
    "devDependencies": {
        "@types/node": "^20.10.6",
        "@types/inquirer": "^8.2.10",
        "@types/fs-extra": "^11.0.4",
        "typescript": "^5.3.3"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "publishConfig": {
        "access": "public"
    }
}